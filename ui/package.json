    "@emotion/styled": "^11.8.1",
    "@faker-js/faker": "^6.1.2",
    "@iconify/react": "^3.1.4",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.4.4",
    "@mui/styled-engine-sc": "^5.4.2",
    "browserify": "^17.0.0",
    "compression": "^1.7.4",
    "date-fns": "^2.28.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "formik": "^2.2.9",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
